<KETL>
	<STEP CLASS="com.kni.etl.ketl.writer.CompressedFileWriter" IMAGE="icons/CompressedWriter.png"
		SMALLIMAGE="icons/CompressedWriterSmall.png">
		<PARAMETERS>
			<PARAMETER NAME="FILEPATH" REQUIRED="TRUE" />
		</PARAMETERS>
		<ATTRIBUTES>
			<ATTRIBUTE NAME="WRITEBUFFER" TYPE="INTEGER" />
			<ATTRIBUTE NAME="LINEFEED">
				<VALUE>DOS</VALUE>
				<VALUE>UNIX</VALUE>
			</ATTRIBUTE>
			<ATTRIBUTE NAME="COMPRESSIONBUFFER" TYPE="INTEGER" />
			<ATTRIBUTE NAME="FORMAT">
				<VALUE>ZIP</VALUE>
				<VALUE>GZIP</VALUE>
			</ATTRIBUTE>
			<ATTRIBUTE NAME="ENABLE_LINEFEED" TYPE="BOOLEAN" />
			<ATTRIBUTE NAME="CHARACTERSET_ATTRIB" TYPE="STRING" />
			<ATTRIBUTE NAME="DELIMITER_AT_END" TYPE="BOOLEAN" />
			<ATTRIBUTE NAME="DELIMITER" TYPE="STRING" />
		</ATTRIBUTES>
		<PORTS>
			<IN>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="DELIMITER" TYPE="STRING" />
					<ATTRIBUTE NAME="FIXEDWIDTH" TYPE="INTEGER" />
					<ATTRIBUTE NAME="MAXIMUMLENGTH" TYPE="INTEGER" />
					<ATTRIBUTE NAME="FORMATSTRING" TYPE="STRING" />
					<ATTRIBUTE NAME="DEFAULTVALUE" TYPE="STRING" />
				</ATTRIBUTES>
			</IN>
		</PORTS>
	</STEP>
</KETL>
