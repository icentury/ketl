#!/bin/bash
# +--------------------------+
# | SETUP JAVA ENVIRONMENT   |
# +--------------------------+


CURRENT_DIR=`pwd`
cd $KETLDIR

. $KETLDIR/conf/SmallExtra.Config

CLASSPATH=$KETL_DIR

$JAVA_HOME/bin/java $JAVA_OPTS \
      -Dlog4j.configuration=file:$KETLDIR/conf/Console.log.properties \
      -classpath $JAVA_HOME/lib/tools.jar:$KETLDIR/lib/KETL.jar \
      Console
cd $CURRENT_DIR

