
# +--------------------------+
# | SETUP JAVA ENVIRONMENT   |
# +--------------------------+


CURRENT_DIR=`pwd`
cd $KETLDIR
CLASSPATH=$KETLDIR

. $KETLDIR/conf/Extra.Config

echo "Starting..."
nohup $JAVA_HOME/bin/java  $JAVA_OPTS \
      -Dlog4j.configuration=file:$KETLDIR/conf/KETL.log.properties \
      -classpath $JAVA_HOME/lib/tools.jar:$KETLDIR/lib/KETL.jar \
      ETLDaemon & 
sleep 2
mv nohup.out $KETLDIR/log/KETL.log 
cd $CURRENT_DIR     
echo "Started."

