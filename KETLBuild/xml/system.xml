	<STEP CLASS="com.kni.etl.ketl.transformation.DimensionTransformation">
		<PARAMETERS>
			<PARAMETER NAME="USER" REQUIRED="TRUE" />
			<PARAMETER NAME="PASSWORD" REQUIRED="TRUE" />
			<PARAMETER NAME="URL" REQUIRED="TRUE" />
			<PARAMETER NAME="DRIVER" REQUIRED="TRUE" />
			<PARAMETER NAME="PRESQL" REQUIRED="FALSE" />
		</PARAMETERS>
		<ATTRIBUTES>
			<ATTRIBUTE NAME="TABLE" DATATYPE="STRING"/>
			<ATTRIBUTE NAME="KEYTABLE" DATATYPE="STRING"/>
			<ATTRIBUTE NAME="SCD" DEFAULT="1">
				<VALUE>1</VALUE>
				<VALUE>2</VALUE>
				<VALUE>3</VALUE>
			</ATTRIBUTE>
		</ATTRIBUTES>
		<PORTS>
			<IN>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="SK" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="INSERT" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="UPDATE" TYPE="BOOLEAN" />
				</ATTRIBUTES>
			</IN>			
			<OUT>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="PK" TYPE="BOOLEAN" />
				</ATTRIBUTES>
			</OUT>
		</PORTS>		
		<TEMPLATES>
			<GROUP NAME="ORACLE">
				<TEMPLATE NAME="MULTIINSERTWRAPPER">insert all ${STATEMENT} select ${VALUES} from dual</TEMPLATE>
				<TEMPLATE NAME="MULTIINSERT">into ${SCHEMANAME}.${TABLENAME}(${DESTINATIONCOLUMNS}) VALUES(${NAMEDVALUES})</TEMPLATE>
				<TEMPLATE NAME="STATEMENTSEPERATOR"></TEMPLATE>
				<TEMPLATE NAME="NAMEDVALUELIST">true</TEMPLATE>
				<TEMPLATE NAME="SEEDLOOKUP">select ${PKCOLUMNS},${SKCOLUMNS} from  ${SCHEMANAME}.${TABLENAME} where mod(${PKCOLUMNS},${PARTITIONS}) = ${PARTITIONID}</TEMPLATE>
			</GROUP>
			<GROUP NAME="DEFAULT">
				<TEMPLATE NAME="NAMEDVALUELIST">false</TEMPLATE>
				<TEMPLATE NAME="STATEMENTSEPERATOR">;</TEMPLATE>
				<TEMPLATE NAME="MULTIINSERTWRAPPER">${STATEMENT}</TEMPLATE>
				<TEMPLATE NAME="CREATEKEYTABLESKINDEX">create unique index SK_${TABLENAME} on ${SCHEMANAME}.${TABLENAME}(${COLUMNS})</TEMPLATE>
				<TEMPLATE NAME="CREATEKEYTABLEPKINDEX">create unique index PK_${TABLENAME} on ${SCHEMANAME}.${TABLENAME}(${COLUMNS})</TEMPLATE>
				<TEMPLATE NAME="CREATEKEYTABLE">create table ${SCHEMANAME}.${KEYTABLENAME} as select distinct ${PK_COLUMNS},${SK_COLUMNS} from ${SCHEMANAME}.${TABLENAME}</TEMPLATE>
				<TEMPLATE NAME="CHECKFORKEYTABLE">select count(*) from ${SCHEMANAME}.${TABLENAME}</TEMPLATE>

				<TEMPLATE NAME="SEEDSELECT">select ${COLUMNS} from ${SCHEMANAME}.${TABLENAME}</TEMPLATE>
				<TEMPLATE NAME="INSERT">insert into ${SCHEMANAME}.${TABLENAME}(${DESTINATIONCOLUMNS}) VALUES(${VALUES})</TEMPLATE>
				<TEMPLATE NAME="MULTIINSERT">insert into ${SCHEMANAME}.${TABLENAME}(${DESTINATIONCOLUMNS}) VALUES(${VALUES})</TEMPLATE>
				<TEMPLATE NAME="SEEDLOOKUP">select ${PKCOLUMNS},${SKCOLUMNS} from  ${SCHEMANAME}.${TABLENAME} where ${PKCOLUMNS} % ${PARTITIONS} = ${PARTITIONID}</TEMPLATE>
			</GROUP>
		</TEMPLATES>
	</STEP>
	
