	<STEP CLASS="com.kni.etl.ketl.writer.JDBCWriter">
		<PARAMETERS>
			<PARAMETER NAME="USER" REQUIRED="TRUE" />
			<PARAMETER NAME="PASSWORD" REQUIRED="TRUE" />
			<PARAMETER NAME="URL" REQUIRED="TRUE" />
			<PARAMETER NAME="DRIVER" REQUIRED="TRUE" />
			<PARAMETER NAME="PRESQL" REQUIRED="FALSE" />
		</PARAMETERS>
		<ATTRIBUTES>
			<ATTRIBUTE NAME="TYPE">
				<VALUE>BULK</VALUE>
				<VALUE>ROLL</VALUE>
				<VALUE>UPSERT</VALUE>
			</ATTRIBUTE>			
			<ATTRIBUTE NAME="IGNOREINVALIDCOLUMNS" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="IQQUOTE" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="STREAMCHANGES" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="REPLACEROWS" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="MANAGEINDEXES" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="HANDLEDUPLICATES" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="INCREMENTALCOMMIT" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="TABLE" TYPE="STRING"/>
			<ATTRIBUTE NAME="RECONNECTONERROR" TYPE="BOOLEAN"/>
			<ATTRIBUTE NAME="HANDLER" TYPE="STRING"/>
			<ATTRIBUTE NAME="JOININDEX" TYPE="STRING"/>
			<ATTRIBUTE NAME="COMMITSIZE" TYPE="INTEGER"/>
		</ATTRIBUTES>
		<PORTS>
			<PRESQL>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="CRITICAL" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="PERPARTITION" TYPE="BOOLEAN" />
				</ATTRIBUTES>
			</PRESQL>
			<PREBATCHSQL>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="CRITICAL" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="PERPARTITION" TYPE="BOOLEAN" />
				</ATTRIBUTES>
			</PREBATCHSQL>
			<IN>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="SK" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="PK" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="COMPARE" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="INSERT" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="WRAP" TYPE="STRING" />
					<ATTRIBUTE NAME="UPDATE" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="ALTERNATE_INSERT_VALUE" TYPE="STRING"/>
					<ATTRIBUTE NAME="ALTERNATE_UPDATE_VALUE" TYPE="STRING"/>
				</ATTRIBUTES>
			</IN>
			<POSTBATCHSQL>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="CRITICAL" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="PERPARTITION" TYPE="BOOLEAN" />
				</ATTRIBUTES>
			</POSTBATCHSQL>
			<POSTSQL>
				<ATTRIBUTES>
					<ATTRIBUTE NAME="CRITICAL" TYPE="BOOLEAN" />
					<ATTRIBUTE NAME="PERPARTITION" TYPE="BOOLEAN" />
				</ATTRIBUTES>
			</POSTSQL>
		</PORTS>		
		<TEMPLATES>
			<GROUP NAME="TERADATA">
				<TEMPLATE NAME="CREATETABLE">create table ${NEWTABLENAME} as (Select ${SOURCECOLUMNS} from ${SOURCETABLENAME} WHERE 1=0) WITH DATA</TEMPLATE>
				<TEMPLATE NAME="MERGETONEWTABLE">CREATE TABLE ${NEWTABLENAME} AS (SELECT ${KEYCOLUMNS} ${OTHERCOLUMNS} FROM ${STGTABLE} as a full outer join ${ORIGTABLE} as b on (${JOIN})) WITH DATA${COMMIT}</TEMPLATE>
				<TEMPLATE NAME="CREATEUNIQUEINDEX">create unique index ${INDEXNAME} (${COLUMNS}) on ${TABLENAME}${COMMIT}</TEMPLATE>
				<TEMPLATE NAME="CREATEINDEX">create index ${INDEXNAME} (${COLUMNS}) on ${TABLENAME}${COMMIT}</TEMPLATE>
				<TEMPLATE NAME="RENAMETABLE">rename table ${TABLENAME} to ${NEWTABLENAME}${COMMIT}</TEMPLATE>
			</GROUP>
			<GROUP NAME="MYSQL">
				<TEMPLATE NAME="STATEMENTSEPERATOR"/>
			</GROUP>			
			<GROUP NAME="POSTGRESQL">
				<TEMPLATE NAME="UPDATECOLUMNFORMAT">${TARGETCOLUMN}=${SOURCECOLUMN}</TEMPLATE>						
				<TEMPLATE NAME="UPDATEPOSTBATCH">update ${DESTINATIONTABLENAME} set ${UPDATECOLUMNS} from ${SOURCETABLENAME} where (${JOIN}) AND exists (Select 1 from ${SOURCETABLENAME} where (${JOIN}) AND (${UPDATETRIGGERS}))</TEMPLATE>
				<TEMPLATE NAME="ANALYZETABLE">analyze ${TABLENAME}</TEMPLATE>
			</GROUP>
			<GROUP NAME="ORACLE">
				<TEMPLATE NAME="ADDHASHCOLUMN">alter table ${TABLENAME} add ${COLUMNNAME} BINARY_FLOAT</TEMPLATE>			
				<TEMPLATE NAME="INSERT">insert /*+ APPEND */ into ${DESTINATIONTABLENAME}(${DESTINATIONCOLUMNS}${DEDUPECOLUMN}) VALUES(${VALUES})</TEMPLATE>
				<TEMPLATE NAME="DISABLEINDEX">alter index ${INDEXNAME} unusable</TEMPLATE>			
				<TEMPLATE NAME="ENABLEINDEX">alter index ${INDEXNAME} rebuild</TEMPLATE>			
				<TEMPLATE NAME="STATEMENTSEPERATOR"/>			
				<TEMPLATE NAME="CREATETABLE">create table ${NEWTABLENAME} as select ${SOURCECOLUMNS}${DEDUPECOLUMN} from ${SOURCETABLENAME} WHERE 1=0</TEMPLATE>
				<TEMPLATE NAME="ANALYZETABLE">analyze table ${TABLENAME} estimate statistics</TEMPLATE>
				<TEMPLATE NAME="INSERTPOSTBATCH">MERGE /*+ APPEND */ INTO ${DESTINATIONTABLENAME} 
  USING ${SOURCETABLENAME}
     ON (${JOIN})
   WHEN MATCHED THEN
    UPDATE SET ${UPDATECOLUMNS}
     WHERE ${UPDATETRIGGERS}
   WHEN NOT MATCHED THEN
    INSERT (${INSERTDESTINATIONCOLUMNS})
     VALUES(${INSERTCOLUMNS})</TEMPLATE>
                <TEMPLATE NAME="UPDATEPOSTBATCH"/>
                <TEMPLATE NAME="DELETEDUPLICATES">delete from ${SOURCETABLENAME} where not exists (SELECT 1 FROM ${SOURCETABLENAME} ${DESTINATIONTABLENAME} where ${JOIN} having max(${DEDUPECOLUMN}) = ${SOURCETABLENAME}.${DEDUPECOLUMN})</TEMPLATE>  
                <TEMPLATE NAME="DELETESTATICROWS">delete from ${SOURCETABLENAME} where (${KEYS}) not in
(SELECT ${SOURCEKEYS}
   FROM ${SOURCETABLENAME} LEFT OUTER JOIN ${DESTINATIONTABLENAME}
         ON (${JOIN})
 WHERE (${FIRSTSK} IS NULL) OR(${JOIN} AND (${UPDATETRIGGERS})))</TEMPLATE>                  
 				<TEMPLATE NAME="DELETETARGET">delete from ${DESTINATIONTABLENAME} where (${KEYS}) IN (SELECT ${KEYS} FROM ${SOURCETABLENAME})</TEMPLATE>                
                <TEMPLATE NAME="INSERTTARGET">insert /*+ APPEND */ into ${DESTINATIONTABLENAME}(${INSERTDESTINATIONCOLUMNS}) SELECT ${INSERTCOLUMNS} FROM ${SOURCETABLENAME}</TEMPLATE>                              
			</GROUP>			
			<GROUP NAME="DEFAULT">
				<TEMPLATE NAME="ADDHASHCOLUMN">alter table ${TABLENAME} add ${COLUMNNAME} int</TEMPLATE>
				<TEMPLATE NAME="UPDATECOLUMNFORMAT">${DESTINATIONTABLENAME}.${TARGETCOLUMN}=${SOURCECOLUMN}</TEMPLATE>						
				<TEMPLATE NAME="DISABLEINDEX"/>			
				<TEMPLATE NAME="ENABLEINDEX"/>			
				<TEMPLATE NAME="ADDHASHCOLUMN">alter table ${TABLENAME} add ${COLUMNNAME} PLS_INTEGER</TEMPLATE>			
				<TEMPLATE NAME="STATEMENTSEPERATOR">;</TEMPLATE>			
				<TEMPLATE NAME="INSERT">insert into ${DESTINATIONTABLENAME}(${DESTINATIONCOLUMNS}${DEDUPECOLUMN}) VALUES(${VALUES})</TEMPLATE>
				<TEMPLATE NAME="CREATETABLE">create table ${NEWTABLENAME} as select ${SOURCECOLUMNS}${DEDUPECOLUMN} from ${SOURCETABLENAME} WHERE 1=0</TEMPLATE>
				<TEMPLATE NAME="SELECTCOLUMNDATATYPE">select ${COLUMNS} from ${TABLENAME} WHERE 1=0</TEMPLATE>
				<TEMPLATE NAME="UPDATEPOSTBATCH">update ${DESTINATIONTABLENAME} inner JOIN ${SOURCETABLENAME} on (${JOIN} AND (${UPDATETRIGGERS})) set ${UPDATECOLUMNS}</TEMPLATE>
				<TEMPLATE NAME="INSERTPOSTBATCH">insert into ${DESTINATIONTABLENAME}(${INSERTDESTINATIONCOLUMNS}) select ${INSERTCOLUMNS} from ${SOURCETABLENAME} where not exists (select 1 from ${DESTINATIONTABLENAME} where ${JOIN})</TEMPLATE>
				<TEMPLATE NAME="DELETEDUPLICATES">delete from ${SOURCETABLENAME} where (${KEYS},${DEDUPECOLUMN}) not in (SELECT ${KEYS},MAX(${DEDUPECOLUMN}) FROM ${SOURCETABLENAME} GROUP BY ${KEYS})</TEMPLATE>
				<TEMPLATE NAME="DROPTABLE">drop table ${TABLENAME}</TEMPLATE>
				<TEMPLATE NAME="TRUNCATETABLE">truncate table ${TABLENAME}</TEMPLATE>
				<TEMPLATE NAME="RENAMETABLE">alter table ${TABLENAME} rename to ${NEWTABLENAME}</TEMPLATE>
				<TEMPLATE NAME="CREATEUNIQUEINDEX">create unique index ${TABLENAME}${INDEXNAME} on ${TABLENAME}(${COLUMNS})</TEMPLATE>
				<TEMPLATE NAME="CREATEINDEX">create index ${TABLENAME}${INDEXNAME} on ${TABLENAME}(${COLUMNS})</TEMPLATE>
				<TEMPLATE NAME="ADDPRIMARYKEY">alter table ${TABLENAME} add primary key(${COLUMNS})</TEMPLATE>
				<TEMPLATE NAME="COALESCE">coalesce(${ARG1},${ARG2})</TEMPLATE>
				<TEMPLATE NAME="ANALYZETABLE">analyze table ${TABLENAME}</TEMPLATE>
				<TEMPLATE NAME="MERGETONEWTABLE">CREATE TABLE ${NEWTABLENAME} AS SELECT ${KEYCOLUMNS} ${OTHERCOLUMNS} FROM ${STGTABLE} as a full outer join ${ORIGTABLE} as b on (${JOIN})</TEMPLATE>
			</GROUP>
		</TEMPLATES>
	</STEP>

	<STEP CLASS="com.kni.etl.ketl.reader.JDBCReader">
