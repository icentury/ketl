#!/bin/bash
# +--------------------------+
# | SETUP JAVA ENVIRONMENT   |
# +--------------------------+
. $KETLDIR/conf/Extra.Config
CLASSPATH=$KETL_DIR

CURRENT_DIR=`pwd`
cd $KETLDIR

$JAVA_HOME/bin/java $JAVA_OPTS \
      -classpath $JAVA_HOME/lib/tools.jar:$KETLDIR/lib/KETL.jar \
      RunJob FILE=$CURRENT_DIR/$1
cd $CURRENT_DIR
