<?xml version="1.0"?>
<ETL xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" VERSION="1.0" xsi:noNamespaceSchemaLocation="F:\Development\XML\ketl.xsd">
<!--  Example: Parse file, split by record type, parse as XML and output
	        Steps: 1. Read file line by line
	               2. Use regular expression matcher to filter records
	               3. Use XML to fields to convert string to xml and extract fields
	               4. Output data to console, db, file etc..
     -->
	<PARAMETER_LIST NAME="myleoOverlapSearchTrackingLogFiles">
 		    <PARAMETER NAME="SEARCHPATH">c:\temp\over\*</PARAMETER>
	</PARAMETER_LIST>

	<PARAMETER_LIST NAME="DestinationDB">
		<PARAMETER NAME="USER">TEST</PARAMETER>
		<PARAMETER NAME="PASSWORD">TEST</PARAMETER>
		<PARAMETER NAME="URL">jdbc:oracle:thin:@localhost:1521:orcl</PARAMETER>
		<PARAMETER NAME="DRIVER">oracle.jdbc.driver.OracleDriver</PARAMETER>
	</PARAMETER_LIST>
	
	<JOB ID="leoOverlapSearchTracking" NAME="leoOverlapSearchTracking" BATCHSIZE="500" PARRALLISM="2" PROJECT="TEST" TYPE="KETL">
		<ACTION>
			<STEP NAME="XMLFileOverlapSearchTracking" ERRORLIMIT="2" IGNOREINVALIDLASTRECORD="TRUE" CLASS="com.kni.etl.ketl.reader.NIOFileReader" CHARACTERSET="UTF-8" PARAMETER_LIST="myleoOverlapSearchTrackingLogFiles">
				<OUT NAME="XML" DATATYPE="STRING"/>
			</STEP>
			
			<!--  Parse field as XML documents and extract fields -->
			<STEP ERRORLIMIT="10" NAME="ExtractOverlapSearchTrackingData" XPATHEVALUATE="FALSE" XPATH="item" CLASS="com.kni.etl.ketl.transformation.XMLToFieldsTransformation">
				<IN XMLDATA="TRUE">XMLFileOverlapSearchTracking.XML</IN>
				<OUT NAME="SESSION_ID" XPATHEVALUATE="FALSE" XPATH="/track/@session_id" DATATYPE="INTEGER"/>
				<OUT NAME="MEMBER_ID" XPATHEVALUATE="FALSE" XPATH="/track/@member_id" DATATYPE="INTEGER"/>
				<OUT NAME="PAGE_SEQ" XPATHEVALUATE="FALSE" XPATH="/track/pageSeq" DATATYPE="INTEGER"/>
				<OUT NAME="Q_PROPOSAL_ACCEPTS" XPATHEVALUATE="FALSE" XPATH="/track/queryProposalAccepts" DATATYPE="INTEGER"/>
				<OUT NAME="ORIGIN" XPATHEVALUATE="FALSE" XPATH="/track/queryOriginType" DATATYPE="CHAR"/>
				<OUT NAME="NUM_RESULTS" XPATHEVALUATE="FALSE" XPATH="/track/numberOfResults" DATATYPE="INTEGER"/>
				<OUT NAME="COMPUTATION_TIME" XPATHEVALUATE="FALSE" XPATH="/track/computationTime" DATATYPE="INTEGER"/>
				<OUT NAME="TRACKING_TIME" XPATHEVALUATE="FALSE" XPATH="/track/@tracking_time" DATATYPE="DATE" FORMATSTRING="e"/>
				<OUT NAME="ORIGIN_NAME_IN" XPATHEVALUATE="FALSE" XPATH="/track/queryOriginName" DATATYPE="STRING"/>
				<OUT NAME="COMPANY_IN" XPATHEVALUATE="FALSE" XPATH="company" DATATYPE="STRING"/>
				<OUT NAME="STARTYEAR" XPATHEVALUATE="FALSE" XPATH="startYear" DATATYPE="INTEGER"/>
				<OUT NAME="ENDYEAR" XPATHEVALUATE="FALSE" XPATH="endYear" DATATYPE="INTEGER"/>
			</STEP>
			<STEP NAME="TransformOverlapSearchTracking" CLASS="com.kni.etl.ketl.transformation.DynamicTransformation">
				<IN>ExtractOverlapSearchTrackingData.*</IN>
				<OUT>${SESSION_ID}</OUT>
				<OUT>${MEMBER_ID}</OUT>
				<OUT>${PAGE_SEQ}</OUT>
				<OUT>${Q_PROPOSAL_ACCEPTS}</OUT>
				<OUT>${ORIGIN}</OUT>
				<OUT>${NUM_RESULTS}</OUT>
				<OUT>${COMPUTATION_TIME}</OUT>
				<OUT>${TRACKING_TIME}</OUT>
				<OUT NAME="ORIGIN_NAME" DATATYPE="STRING">${ORIGIN_NAME_IN}!=null &amp;&amp;${ORIGIN_NAME_IN}.length()>50?${ORIGIN_NAME_IN}.substring(0,50):${ORIGIN_NAME_IN}</OUT>
				<OUT>${STARTYEAR}</OUT>
				<OUT>${ENDYEAR}</OUT>	
				<OUT NAME="COMPANY" DATATYPE="STRING">${COMPANY_IN}!=null &amp;&amp; ${COMPANY_IN}.length()>50?${COMPANY_IN}.substring(0,50):${COMPANY_IN}</OUT>
			</STEP>	
			<STEP NAME="GroupOverlapSearchRecord" CLASS="com.kni.etl.ketl.transformation.Aggregate">
				<IN>TransformOverlapSearchTracking.SESSION_ID</IN>
				<IN>TransformOverlapSearchTracking.MEMBER_ID</IN>
				<IN>TransformOverlapSearchTracking.PAGE_SEQ</IN>
				<IN>TransformOverlapSearchTracking.Q_PROPOSAL_ACCEPTS</IN>
				<IN>TransformOverlapSearchTracking.ORIGIN</IN>
				<IN>TransformOverlapSearchTracking.NUM_RESULTS</IN>
				<IN>TransformOverlapSearchTracking.COMPUTATION_TIME</IN>
				<IN>TransformOverlapSearchTracking.TRACKING_TIME</IN>
				<IN>TransformOverlapSearchTracking.ORIGIN_NAME</IN>
				<IN>TransformOverlapSearchTracking.COMPANY</IN>
				<IN>TransformOverlapSearchTracking.STARTYEAR</IN>
				<IN>TransformOverlapSearchTracking.ENDYEAR</IN>
				<OUT>${SESSION_ID}</OUT>
				<OUT>${MEMBER_ID}</OUT>
				<OUT>${PAGE_SEQ}</OUT>
				<OUT>${Q_PROPOSAL_ACCEPTS}</OUT>
				<OUT>${ORIGIN}</OUT>
				<OUT>${NUM_RESULTS}</OUT>
				<OUT>${COMPUTATION_TIME}</OUT>
				<OUT>${TRACKING_TIME}</OUT>
				<OUT>${ORIGIN_NAME}</OUT>
				<OUT NAME="Q_ENTRIES" FUNCTION="ToArray" MAXELEMENTS="20000" DATATYPE="ora_array.QueryEntryDBO">new ora_array.QueryEntryDBO(${COMPANY},${STARTYEAR},${ENDYEAR})</OUT>
			</STEP>
			<STEP NAME="InsertOverlapSearchRecord" CLASS="com.kni.etl.ketl.writer.JDBCWriter" COMMITSIZE="1000" HANDLER="com.kni.etl.ketl.dbutils.oracle.OracleJDBCItemHelper" TABLE="OVERLAP_SEARCH_TRACKING" PARAMETER_LIST="DestinationDB" ERRORLIMIT="10000">
				<PRESQL>truncate table OVERLAP_SEARCH_TRACKING</PRESQL>
				<IN>GroupOverlapSearchRecord.SESSION_ID</IN>
				<IN>GroupOverlapSearchRecord.MEMBER_ID</IN>
				<IN>GroupOverlapSearchRecord.PAGE_SEQ</IN>
				<IN>GroupOverlapSearchRecord.Q_PROPOSAL_ACCEPTS</IN>
				<IN>GroupOverlapSearchRecord.ORIGIN</IN>
				<IN>GroupOverlapSearchRecord.NUM_RESULTS</IN>
				<IN>GroupOverlapSearchRecord.COMPUTATION_TIME</IN> -->
				<IN>GroupOverlapSearchRecord.TRACKING_TIME</IN>
				<IN>GroupOverlapSearchRecord.ORIGIN_NAME</IN>
				<IN ARRAYTYPENAME="QUERY_ENTRY_ARRAY_T">GroupOverlapSearchRecord.Q_ENTRIES</IN> 
			</STEP>
		</ACTION>
	</JOB>
</ETL>
